{
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "api/index.js",
        "methods": ["GET", "POST"]
      }
    ]
  }
  

